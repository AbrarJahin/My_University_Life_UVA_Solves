<BR><TEXTAREA rows=3D40 readOnly cols=3D85>To find the BFS and DFS of =
the given graph


#include&lt;stdio.h&gt;
#include&lt;conio.h&gt;

#define size 20

int a[10][10],vertex[10],n,e;

/*STACK FUNCTIONS*/
#define bottom -1

int stack[size],top=3Dbottom;
int stackempty()
{
	return(top=3Dbottom) ? 1:0;
}
int stackfull()
{
	return(top=3D=3Dsize-1) ? 1:0;
}

void push(int item)
{
	if(stackfull())
		printf("7

 STACK IS FULL");
	else
		stack[++top]=3Ditem;
}

int pop()
{
	if(stackempty())
	{
		printf("7

 STACK IS EMPTY");
		return -1;
	}
	else
		return stack[top--];
}

int peep()
{
	if(stackempty())
	{
		printf("7

 STACK IS EMPTY");
		return -1;
	}
	else
		return stack[top];
}

/* QUEUE FUNCTIONS */
#define start -1

int q[size];
int f=3Dstart,r=3Dstart;

int qempty(){ return(f=3D=3Dr)?1:0; }
int qfull(){ return(r=3D=3Dsize-1)?1:0;}

void addq(int c)
{
if(qfull())
	printf("7

QUEUE IS FULL");
else
	q[++r]=3Dc;
}

int delq()
{
if(qempty())
	{
	printf("7

QUEUE IS EMPTY");
	return -1;
	}
else
	return q[++f];
}
// j is unvisited adjecent vertex to i
int adjvertex(int i)
{
int j;
for(j=3D0;j&lt;n;j++)
	if(a[i][j]=3D=3D1&amp;&amp;vertex[j]=3D=3D0)
		return j;
	return n;
}

int  visitall()
{
int i;
for(i=3D0;i&lt;n;i++)
	if(vertex[i]=3D=3D0)
		return 0;
	return 1;
}

/*FUNCTION FOR BFS*/
void bfs()
{
int i,j,k,cur=3D0;//current vertex is startting vertex
for(i=3D0;i&lt;n;i++)
	vertex[i]=3D0;//not visited
printf("

BFS path =3D&gt; V%d ",cur+1);
addq(cur);
vertex[cur]=3D1;//marking visited vertex
	while(!visitall())
		{
		if(qempty())
			{
			printf("7

GRAPH IS DISCONNECTED");
			break;
			}
		cur=3Ddelq();
		//visit all vertices which are adjecent to current vertex
		for(j=3D0;j&lt;n;j++)
			{
			//adjecent are not visited
			if(a[cur][j]=3D=3D1&amp;&amp;vertex[j]=3D=3D0)
				{
				printf("V%d ",j+1);
				addq(j);
				//marking visited vertex
				vertex[j]=3D1;
				}
			}
		}
}

/*FUNCTION FOR DFS*/
void dfs()
{
int i,j,k,cur=3D0;//current vertex is startting vertex
for(i=3D0;i&lt;n;i++)
	vertex[i]=3D0;//not visited
printf("

DFS path =3D&gt; V%d ",cur+1);
push(cur);
vertex[cur]=3D1;//marking visited vertex
	while(!visitall())
		{
		do
			{
			cur=3Dadjvertex(peep());
			if(cur=3D=3Dn) pop();
			}
		while(cur=3D=3Dn&amp;&amp;!stackempty());
		if(stackempty())
			{
			printf("7

GRAPH IS DISCONNECTED");
			break;
			}
		if(cur!=3Dn)
			{
			printf(" V%d ",cur+1);
			push(cur);
			vertex[cur]=3D1;//marking visited vertex
			}
		}
}

/*MAIN PROGRAM*/
void main()
{
	int i,j,k;
	clrscr();
	for(i=3D0;i&lt;10;i++)
	for(j=3D0;j&lt;10;j++)
	a[i][j]=3D0;

	printf("
ENTER NO OF VERTICES &amp; EDGES OF UNDIRECTED GRAPH : ");
	scanf("%d%d",&amp;n,&amp;e);

	printf("
ENTER EDGES AS PAIR OF VERTICES

");
	for(k=3D1;k&lt;=3De;k++)
		{
		printf("EDGE %d =3D&gt;",k);
		scanf("%d%d",&amp;i,&amp;j);
		//for undirected graph
		a[i-1][j-1]=3D1;
		}

	dfs();
	bfs();
	getch();
}




