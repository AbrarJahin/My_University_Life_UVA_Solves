	pair&lt;map&lt;Net, h_DM&gt;::iterator, <VAR>bool</VAR>&gt; r;
	r =3D route_list.insert(pair&lt;<VAR>const</VAR> Net, h_DM&gt;(n, h));
	route_cost.insert(pair&lt;<VAR>const</VAR> Net, <VAR>int</VAR>&gt;(n, =
cost));
	<VAR>if</VAR>(r.second =3D=3D <VAR>false</VAR>) {
